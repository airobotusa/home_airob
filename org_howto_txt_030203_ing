/* heat_slotr.prog */
CREATOR              :  ""
MODIFYTIME           :  ""
ACCESS               :  ""
VERSION              :  ""
TEMPLATE_NAME        :  heat_slotr
DESCRIPTION          : "This program is used for slot delay of chamber"
/* process type should be service !! */
PROCESS_TYPE         : PROCESSING
CHAMBER_CLASS        : chmbHeat
PARAMETER_DEFINITIONS
/*
   This section contains definitions of attributes that are set
   once in a process, before or after the step section. One value
   is recieved from the recipe for the setting of the attribute.
   Attributes may be grouped for MMI display purposes.
*/
   GENERAL_SETTABLE_PARAMETERS
          DEFINE: max_recipe_time process:x;max_recipe_time
          DESCRIPTION : ""
          MIN         : 0
      MAX         : 36000000
          DEFAULT     : 3600000
      DEFINE          : wafer_history process:x;bool_9
      DESCRIPTION     : "control data collection"
      MIN             : NA
      MAX             : NA
          DEFAULT         : FALSE
/*
   This section contains definitions of attributes that are set
   in each step. values are recieved from the recipe for the setting
   of the attribute, one value for each step
   Attributes may be grouped for MMI display purposes.
*/
   STEP_SETTABLE_PARAMETERS
          DEFINE: max_step_time process:x;max_step_time
          DESCRIPTION : ""
          MIN         : 0
      MAX         : 36000000
          DEFAULT     : 3600000
          DEFINE: atm_slot_delay process:x;time_1
          DESCRIPTION : " "
          MIN         : 1000
      MAX         : 30000000
          DEFAULT     : 3000
          DEFINE: vac_slot_delay process:x;time_2
          DESCRIPTION : " "
          MIN         : 1000
      MAX         : 30000000
          DEFAULT     : 420000
/*
   This section describes the attributes to be used for internal
   use of the program. These attributes do not recieve values from
   the recipe. They are either set with values specified in the program
   or, are used in conditions
*/
    INTERNAL_PARAMETERS
         DEFINE: pressure_mode  ;pressure_mode
         DEFINE: current_temp   tempdevMultiht:htr_multi;temp
         DEFINE: slot_name  process:x;instname1
         DEFINE: attr_name  process:x;attrname1
         DEFINE: slot_delay process:x;value1
         DEFINE: current_slot   slotMgr:x;current_slot_path
/*
   This section describes the attributes to be monitored by the process
   control. Alarm and warning tolerances are defined as well as the name of
   the attribute that the tolerances should be compared to.
*/
   PROCESS_MONITORED_PARAMETERS
/*
   This section describes the attributes to be monitored by the MMI.
   Attributes may be grouped for MMI display purposes.
*/
   DISPLAYED_PARAMETERS
/*
   This section describes the predefined endpoints.
   Endpoint may be have parameters Values for the parameters are recieved
   from the recipe. The recipe defines an end point for each step.
*/
  PREDEFINED_ENDPOINT_DEFINITIONS
   ENDPOINT_NAME :  no_wait
     DESCRIPTION   :  "no wait"
          ENDPOINT_BODY
              ( max_recipe_time == max_recipe_time )
          ENDPOINT_END
/****************************************************************************/
PROGRAM_DEFINITION
/*** SET OF GENERAL PARAMETERS ***/
   SET      max_recipe_time
   STEP_SECTION_BEGIN
   SET      max_step_time
   SET      wafer_history
   SET      slot_name, current_slot
   SET      attr_name, "slot_delay"
   IF ( wafer_history == ENUM(TRUE) )
      START_DATA_COLLECTION
   ENDIF
   IF ( ( pressure_mode == ENUM (atmospheric) ) or ( current_temp < 100 ) )
      SET   atm_slot_delay
      SET   slot_delay, atm_slot_delay
   ELSE
      SET   vac_slot_delay
      SET   slot_delay, vac_slot_delay
   ENDIF
   IF ( wafer_history == ENUM(TRUE) )
      STOP_DATA_COLLECTION
   ENDIF
   DETECT             ENDPOINT
   STEP_SECTION_END
/*
  FAULT_EXCEPTION
   EXCEPTION_END
   ABORT_EXCEPTION
   EXCEPTION_END
   TIMEOUT_EXCEPTION
   EXCEPTION_END
*/
PROGRAM_END

