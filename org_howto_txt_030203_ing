   CREATOR           : Undefined
   PASSWORD          : Undefined
   DESCRIPTION       : ""
   CHAMBER_CLASS     : chmbLLVac
 GENERAL_PARAMETERS_SETTINGS :
		max_recipe_time = 900000
	STEP_NAME : None
		max_step_time = 800000
		vacuum_set_point = 500.00
		END_POINT  : no_wait
	STEP_END
END_PROCESS_RECIPE



template: pump
---------------
TEMPLATE_NAME        :  pump
DESCRIPTION          : "This program is used for pumping ll chamber"
PROCESS_TYPE         : SERVICE
CHAMBER_CLASS        : chmbLLVac
   GENERAL_SETTABLE_PARAMETERS
	 DEFINE: max_recipe_time process:x;max_recipe_time
   DESCRIPTION : "maximum milliseconds for recipe before timeout error"
	 MIN         : 1000
   MAX         : 1000000
	 DEFAULT     : 900000
/****************************************************************************/
PROGRAM_DEFINITION
/*** SET OF GENERAL PARAMETERS ***/
   SET            max_recipe_time
   STEP_SECTION_BEGIN
   SET            max_step_time
   /* Check if door is closed */
   IF ( door_pos !=  ENUM ( close ) )
    EVENT   "Cannot pump when door is open"
    DETECT FAULT
   ENDIF
   /* Pump */
   SET slow_vent, ENUM ( closed )
   SET fast_vent, ENUM ( closed )
      /** turn pump on **/
      IF ( pump_status == ENUM ( OFF ) )
         SET pump_activity   , ENUM ( TURN_ON )
      ENDIF
      /** wait for pump to be turned on. if it was turned on
          by heat chmb, we wait till its running **/
      WAIT ( pump_status == ENUM ( RUNNING ) )
   /* dshen -- added next two lines to reset vacuum state.
    In case someone manually changed vac valves.  */
   SET     vacuum_activity , ENUM ( ISOLATE )
   WAIT    ( vacuum_gen_state == ENUM(ready))
   /* dshen -- check all vac valves again. ISOLATE command does not close
                  manually opened vac valves. 5/98                        */
      IF (slow_vac_valve != ENUM( closed ))
         SET slow_vac_valve, ENUM( closed )
      ENDIF
      IF (fast_vac_valve != ENUM( closed ))
         SET fast_vac_valve, ENUM( closed )
      ENDIF
   SET          vacuum_set_point
   SET      vacuum_activity , ENUM ( PUMP )
   WAIT     ( vacuum_stability ==  ENUM (  stable ) )
   DELAY    2000
   DETECT ENDPOINT
   STEP_SECTION_END
   FAULT_EXCEPTION
      EVENT      "fault was detected in pump service"
      SET        vacuum_activity    , ENUM ( ISOLATE )
    EXCEPTION_END
   ABORT_EXCEPTION
      EVENT      "Pump service aborted"
      SET        vacuum_activity    , ENUM ( ISOLATE )
    EXCEPTION_END
   TIMEOUT_EXCEPTION
      EVENT      "Pump service timed out"
      SET        vacuum_activity    , ENUM ( ISOLATE )
    EXCEPTION_END
PROGRAM_END

for example
ll_pump
1. door close
2. pump check/on
3. Vac valve
4. user defined valve close
   ------------

