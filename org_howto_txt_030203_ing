	DESCRIPTION     : "Delay after reaching process position before reaching spa cing"
	MIN             : 0
	MAX             : 20000
	DEFAULT         : 0
make sure time_x is not used anywhere except here

STEP_SECTION_BEGIN
SET     process_pos_delay

/* yhwang added for delay after getting process_offset */
IF (step_num == 1)
		EVENT "before delay in first step"
		DELAY   process_pos_delay
		EVENT "after in first step"
ENDIF
SET     Process_spacing

- how to make loop for rpsc cleaning for certain steps' repeat period
in cvdb_rpsc_cyc.prog which is copied from cvdb_rpsc_epd.prog

GENERAL_SETTABLE_PARAMETERS
	DEFINE          : last_step                 process:x;int32_17
	DESCRIPTION     : "last step to repeat on"
	MIN             : 0
	MAX             : 100
	DEFAULT         : 18
	DEFINE          : repeat_over               process:x;int32_18
	DESCRIPTION     : "last step to repeat on"
	MIN             : 0
	MAX             : 2000
	DEFAULT         : 18

PROGRAM_DEFINITION
/*** Header Section for general parameters ***/
SET     total_step,0
SET     repeat_over
SET     last_step

SET  step_end_done, ENUM (TRUE)
==>
      IF ( (current_step == last_step) AND (total_step < repeat_over))
			  SET     total_step,total_step + 1
			  EVENT   " REPEATING OVER ",total_step
			  SET     current_step, 1
	  ENDIF
DETECT ENDPOINT
STEP_SECTION_END

INTERNAL_PARAMETERS
        DEFINE: total_step process:x;int32_16
		DEFINE: current_step process:x;step

