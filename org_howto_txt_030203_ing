- how to GetAttr which is running VME CPU from SUN UNIX
use commandCmdGetAttr
if ( instSetFixedID ( wh_chmb->chmb_id,
	 chmbSLOTMGR_X_FIXED_SUB_INSTANCE_ID,
	&slotMgr_id )      != instOK )
{
   errorReportAndReturn0 ( whFAILURE, "can't find id of slot mgr" );
}
/* use commandCmdGetAttr because wh is running in SUN not CPU */
if (commandCmdGetAttr(slotMgr_id, slotMgrSLOT_PROCESS_DATA_ATTR_ID,
					 &wafer_header->pdc) != commandOK)
{
   errorReportAndReturn0(whFAILURE, "Fail to get SLOT_PROCESS_DATA");
}

